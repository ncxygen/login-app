      },
      "package": "com.xygen.login",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "myapp",
    "extra": {
      "eas": {
        "projectId": "5ab25165-40d5-4f0a-9fb7-4ad2a083f006"
      }
    },
    "owner": "nicolasxygen"
